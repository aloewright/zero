        }
      ]
    },
    "staging": {
      "name": "zero-inbound-mail-staging",
      "vars": {
        "FORWARD_TO": ""
      },
      "d1_databases": [
        {
          "binding": "INBOUND_DB",
          "database_name": "zero-inbound-mail-staging",
          "database_id": "f12b8b51-3932-419f-b382-6dbec6169de3"
        }
      ],
      "r2_buckets": [
        {
          "binding": "INBOUND_BUCKET",
          "bucket_name": "zero-inbound-mail-staging"
